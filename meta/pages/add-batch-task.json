{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${batchtask.Name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameRequiredValidator",
                              "component": "Name",
                              "text": "Name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${batchtask.Description}"
                            },
                            {
                              "type": "required-validator",
                              "name": "DescriptionRequiredValidator",
                              "component": "Description",
                              "text": "Description is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Parameters",
                              "component": "Parameters",
                              "name": "ParametersLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 100,
                              "type": "textbox",
                              "name": "Parameters",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${batchtask.Parameters}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ParametersRequiredValidator",
                              "component": "Parameters",
                              "text": "Parameters is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Interval Minutes",
                              "component": "IntervalMinutes",
                              "name": "IntervalMinutesLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "IntervalMinutes",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${batchtask.IntervalMinutes}"
                            },
                            {
                              "type": "required-validator",
                              "name": "IntervalMinutesRequiredValidator",
                              "component": "IntervalMinutes",
                              "text": "IntervalMinutes is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Is Running",
                              "component": "IsRunning",
                              "name": "IsRunningLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool",
                              "type": "checkbox",
                              "name": "IsRunning",
                              "style": {
                                "display": "block"
                              },
                              "value": "${batchtask.IsRunning}"
                            },
                            {
                              "type": "required-validator",
                              "name": "IsRunningRequiredValidator",
                              "component": "IsRunning",
                              "text": "IsRunning is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Last Ejecution",
                              "component": "LastEjecution",
                              "name": "LastEjecutionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "type": "datepicker",
                              "name": "LastEjecution",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${batchtask.LastEjecution}"
                            },
                            {
                              "type": "required-validator",
                              "name": "LastEjecutionRequiredValidator",
                              "component": "LastEjecution",
                              "text": "LastEjecution is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Is Active",
                              "component": "IsActive",
                              "name": "IsActiveLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool",
                              "type": "checkbox",
                              "name": "IsActive",
                              "style": {
                                "display": "block"
                              },
                              "value": "${batchtask.IsActive}"
                            },
                            {
                              "type": "required-validator",
                              "name": "IsActiveRequiredValidator",
                              "component": "IsActive",
                              "text": "IsActive is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Creation Date",
                              "component": "CreationDate",
                              "name": "CreationDateLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "type": "datepicker",
                              "name": "CreationDate",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${batchtask.CreationDate}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CreationDateRequiredValidator",
                              "component": "CreationDate",
                              "text": "CreationDate is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new BatchTask!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ManagerImage.createBatchTask",
                        "parameters": [
                          {
                            "name": "BatchTask",
                            "value": "${batchtask}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${batchtask}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${batchtask}",
                  "visible": "${batchtask != null}",
                  "itemType": "ManagerImage.Models.ManagerImage.BatchTask"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "batchtask",
        "type": "set",
        "value": "new ManagerImage.Models.ManagerImage.BatchTask()",
        "valueType": "ManagerImage.Models.ManagerImage.BatchTask"
      }
    ]
  },
  "name": "Add Batch Task",
  "layout": "Main"
}