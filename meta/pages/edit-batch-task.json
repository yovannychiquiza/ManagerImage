  "events": {
    "load": [
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "error": [],
        "name": "ManagerImage.getBatchTaskById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
          {
            "name": "batchtask",
            "type": "set",
            "value": "${result}",
            "valueType": "ManagerImage.Models.ManagerImage.BatchTask"
          }
        ],
        "type": "invoke"
      }
    ]
